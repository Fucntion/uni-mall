<view class="container"><view class="carousel-section"><view class="titleNview-placing"></view><view class="titleNview-background" style="{{'background-color:'+(titleNViewBackground)+';'}}"></view><swiper class="carousel" circular autoplay="true" duration="1000" data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{carouselList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item data-event-opts="{{[['tap',[['navToDetailPage',[['o',['title','轮播广告']]]]]]]}}" class="carousel-item" bindtap="__e"><image src="{{item.src}}"></image></swiper-item></block></swiper><view class="swiper-dots"><text class="num">{{swiperCurrent+1}}</text><text class="sign">/</text><text class="num">{{swiperLength}}</text></view></view><view class="cate-section"><view data-event-opts="{{[['tap',[['navToList',['新品推荐']]]]]}}" class="cate-item" bindtap="__e"><image src="/static/temp/shortcut-new.png"></image><text>新品推荐</text></view><view data-event-opts="{{[['tap',[['navToList',['热销市场']]]]]}}" class="cate-item" bindtap="__e"><image src="/static/temp/shortcut-hot.png"></image><text>热销市场</text></view><view data-event-opts="{{[['tap',[['navToList',['促销折扣']]]]]}}" class="cate-item" bindtap="__e"><image src="/static/temp/shortcut-discount.png"></image><text>促销折扣</text></view><view class="cate-item"><image src="/static/temp/shortcut-brand.png"></image><text>品牌街</text></view></view><view class="seckill-section m-t"><view data-event-opts="{{[['tap',[['navToList',['爆款商品']]]]]}}" class="s-header" bindtap="__e"><image class="s-img" src="/static/icon-fire.png" mode="widthFix"></image><text class="tip">爆款商品</text></view><scroll-view class="floor-list" scroll-x><view class="scoll-wrapper"><block wx:for="{{seckillGoodsList}}" wx:for-item="goods" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['seckillGoodsList','',index]]]]]]]}}" class="floor-item" bindtap="__e"><view class="image-wrapper"><image class="{{[goods.loaded]}}" src="{{goods.DefaultPicURL}}" mode="aspectFill" lazy-load data-event-opts="{{[['load',[['onImageLoad',['seckillGoodsList',index]]]],['error',[['onImageError',['seckillGoodsList',index]]]]]}}" bindload="__e" binderror="__e"></image></view><text class="title clamp">{{goods.GoodsName}}</text><text class="subtitle clamp">{{goods.GoodsModel}}</text><text class="price">{{"￥"+goods.Price}}</text></view></block></view></scroll-view></view><block wx:for="{{mallGoodsList}}" wx:for-item="mallGoods" wx:for-index="index" wx:key="index"><block><view data-event-opts="{{[['tap',[['navToList',['$0'],[[['mallGoodsList','',index,'GoodsClassName']]]]]]]}}" class="f-header m-t" bindtap="__e"><image src="/static/temp/h1.png"></image><view class="tit-box"><text class="tit">{{mallGoods.GoodsClassName}}</text></view><text class="yticon icon-you"></text></view><view class="class-floor"><view class="guess-section"><block wx:for="{{mallGoods.MallGoodsList}}" wx:for-item="goods" wx:for-index="index2" wx:key="index2"><view data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['mallGoodsList','',index],['MallGoodsList','',index2]]]]]]]}}" class="guess-item" bindtap="__e"><view class="image-wrapper"><image class="{{[goods.loaded]}}" src="{{goods.DefaultPicURL}}" mode="aspectFill" lazy-load data-event-opts="{{[['load',[['onFloorImageLoad',['mallGoodsList',index,index2]]]],['error',[['onFloorImageError',['mallGoodsList',index,index2]]]]]}}" bindload="__e" binderror="__e"></image></view><text class="title clamp">{{goods.GoodsName}}</text><text class="subtitle clamp">{{"型号："+goods.GoodsModel}}</text><text class="price">{{"￥"+goods.Price}}</text></view></block></view></view></block></block></view>